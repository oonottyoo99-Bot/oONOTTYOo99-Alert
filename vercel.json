{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/index", "dest": "/api/index.py" },
    { "src": "/api/hello", "dest": "/api/hello.py" },
    { "src": "/api/health", "dest": "/api/health.py" }
  ]
}
